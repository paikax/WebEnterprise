@model IEnumerable<WebEnterprise.Models.User>

<h1>User Management</h1>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>Full Name</th>
            <th>Gender</th>
            <th>Date of Birth</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.FullName</td>
                <td>@user.Gender</td>
                <td>@user.DoB</td>
                <td>
                    <a asp-controller="Users" asp-action="Edit" asp-route-id="@user.Id">Edit</a> |
                    <a asp-controller="Users" asp-action="Details" asp-route-id="@user.Id">Details</a> |
                    <a asp-controller="Users" asp-action="Delete" asp-route-id="@user.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
